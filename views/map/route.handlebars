<h3> Find your way </h3>

<div id="map"></div>
<div id="floating-panel">

<form action="/route" method="post" class="checkRoute">
  <label for="start location"><b>Start bldg: </b></label>
  <input type="text" name="start" id="field1" size="35" placeholder="Building name/Address/Postal code">
  <br>
  <label for="end location"><b>End bldg: </b></label>
  <input type="text" name="end" id="field2" size="35" placeholder="Building name/Address/Postal code">
  <br>
  <button type="submit" name="button" id="check">Check routes</button>
</form>

</div>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCBGZHiiqZ9lmXUvxe5h-v-gCHC1lnZl5c&callback=initMap&language=en&region=SG&libraries=places"
  async defer></script>
<script src="/assets/js/mapscript.js"></script>
